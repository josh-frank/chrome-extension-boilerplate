"manifest_version": 2,
"name": "Top Posts Viewer",
"version": "1.0",
"description": "Launch a new tab with top posts from any subreddit.",
"permissions": [
    "contextMenus",
    "tabs"
],
"background": {
    "scripts": [ "js/background.js" ]
},
"browser_action" : {
    "default_icon" : "icon.png"
},
"icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128":"icon.png"
   }
},